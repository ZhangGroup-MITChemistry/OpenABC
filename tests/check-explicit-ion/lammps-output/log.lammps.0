############################ ion related potential, Na+: 35, Mg2+: 36, Cl-: 37 #####################################
# Specifying the ion parameters from Freeman et al. JCP, 135:165104, 2011.
variable myDielectric equal 78
dielectric ${myDielectric}
dielectric 78

#Turnin off pair interactions from 3SPN2
pair_coeff 1 1 none
pair_coeff 1 35*37 none
pair_coeff 2*37 35*37 none

# Apply LJ attraction between ion sites (Here, we do not define LJ repulsion for 16,18,20,26 protein residues, we will do it in the following section)
pair_coeff 1 1 lj/cut  0.18379 6.86 # P-Na+
pair_coeff 1 35 lj/cut  0.02510 4.14 # P-Na+
pair_coeff 1 36 lj/cut  0.11950 4.87 # P-Mg#+
pair_coeff 1 37 lj/cut  0.08121 5.5425 # P-Cl-
pair_coeff 35 35 lj/cut 0.01121 2.43 # Na+-Na+
pair_coeff 35 36 lj/cut 0.04971 2.37 # Na+-Mg2+
pair_coeff 35 37 lj/cut 0.08387 3.1352 # Na+-Cl-
pair_coeff 36 36 lj/cut 0.89460 1.412 # Mg2+-Mg2+
pair_coeff 36 37 lj/cut 0.49737 4.74 # Mg2+-Cl-
pair_coeff 37 37 lj/cut 0.03585 4.045 # Cl--Cl-

# Apply coul/long between all ions (solve using ewald sum)
pair_coeff 1 1 coul/long
pair_coeff 1 16 coul/long
pair_coeff 1 18 coul/long
pair_coeff 1 20 coul/long
pair_coeff 1 26 coul/long
pair_coeff 16 16 coul/long
pair_coeff 16 18 coul/long
pair_coeff 16 20 coul/long
pair_coeff 16 26 coul/long
pair_coeff 18 18 coul/long
pair_coeff 18 20 coul/long
pair_coeff 18 26 coul/long
pair_coeff 20 20 coul/long
pair_coeff 20 26 coul/long
pair_coeff 26 26 coul/long

pair_coeff 1 35*37 coul/long
pair_coeff 16 35*37 coul/long
pair_coeff 18 35*37 coul/long
pair_coeff 20 35*37 coul/long
pair_coeff 26 35*37 coul/long
pair_coeff 35*37 35*37 coul/long

kspace_style pppm 1.0e-4

